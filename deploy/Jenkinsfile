pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                script {
                    sh 'echo "Building Stage.."'
                }
            }
        }
        stage('Testing') {
            steps {
                script {
                    sh 'echo "Testing Stage.."'
                }
            }
        }
        stage('Package') {
            steps {
                script {
                    sh 'echo "Packaging App Stage.."'
                }
            }
        }
        stage('Destroy') {
            steps {
                script {
                    sh 'echo "Destroy Stage.."'
                }
            }
        }
        stage('Clean up') {
            steps {
                script {
                    sh 'echo "Clean up Stage.."'
                }
            }
        }
    }
}
